{
	"name": "better-admin",
	"version": "0.0.1",
	"type": "module",
	"description": "CLI for installing Better Admin components with automatic shadcn/ui dependency resolution",
	"author": "armelgeek",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/armelgeek/better-query.git",
		"directory": "packages/better-admin"
	},
	"keywords": [
		"admin",
		"cli",
		"components",
		"shadcn",
		"ui",
		"react"
	],
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"build": "tsup --clean",
		"dev": "tsup --watch",
		"test": "vitest",
		"typecheck": "tsc --noEmit"
	},
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"bin": {
		"better-admin": "./dist/cli/index.js"
	},
	"exports": {
		".": "./dist/index.js",
		"./cli": "./dist/cli/index.js"
	},
	"dependencies": {
		"commander": "^11.1.0",
		"prompts": "^2.4.2",
		"chalk": "^5.3.0",
		"ora": "^8.0.1",
		"node-fetch": "^3.3.2",
		"fs-extra": "^11.2.0",
		"execa": "^9.0.0"
	},
	"peerDependencies": {
		"react": "^18.0.0 || ^19.0.0"
	},
	"devDependencies": {
		"@types/prompts": "^2.4.9",
		"@types/fs-extra": "^11.0.4",
		"@types/react": "^18.2.0",
		"react": "^18.2.0",
		"tsup": "^8.2.4",
		"typescript": "^5.5.0",
		"vitest": "^1.6.0"
	},
	"files": [
		"dist",
		"registry",
		"CHANGELOG.md",
		"README.md"
	]
}
